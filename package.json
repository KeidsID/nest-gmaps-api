{
  "name": "nest-gmaps-api",
  "version": "0.0.1",
  "description": "Simple back-end that act as Google Maps API proxy",
  "private": true,
  "type": "module",
  "engineStrict": true,
  "engines": {
    "node": ">=20.18.0"
  },
  "license": "MIT",
  "author": {
    "name": "Kemal Idris",
    "email": "keidscode@gmail.com",
    "url": "https://github.com/KeidsID"
  },
  "scripts": {
    "build": "nest build && tsc-alias -p tsconfig.build.json",
    "start": "npm run build && node build/main.js",
    "start:dev": "nest start --watch -e \"tsc-alias -p tsconfig.build.json && node build/main.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "test:e2e": "jest --config ./test/jest.e2e.config.js --coverage"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged --cwd .",
    "commit-msg": "npx @commitlint/cli --edit"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "danger": "^12.3.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.2",
    "prettier": "^3.4.2",
    "simple-git-hooks": "^2.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
